function init_game()
	percent=100
	init_background()
	hand=create_hand({x=46,y=128})
	coin=create_coin({x=56,y=64})
	score=0
end

function update_hand_position()
	if bg_status==0 and coin.position.y>=0 then
		hand.position.y=max(
			hand.position.y-offset_speed,
			56
		)
	else
		hand.position.y=min(
			hand.position.y+offset_speed,
			128
		)
	end
end

function update_game()
	update_background(
		function() --space
			
		end,
		function() --sky
			local percent=update_progress()
			update_clouds(coin,percent,function()
				score=max(score-1,0)
			end)
			update_baloons(coin,percent,function()
				score+=1
			end)
			if btn(âž¡ï¸) then
				coin.velocity.x=min(coin.velocity.x+1,3)
			elseif btn(â¬…ï¸) then
				coin.velocity.x=max(coin.velocity.x-1,-3)
			else
				coin.velocity.x*=0.95
			end
			if coin.position.y>128 then
				coin.position.y=-128
				coin.position.x=56
				coin.velocity.y=0
				coin.velocity.x=0
				bg_status=0
			end
		end,
		function() --ground
			update_hand(hand,function(power)
				coin.velocity.y=-max(power,2.5)
				result=rnd({0,1})
			end)
			if coin.position.y<-16 and coin.velocity.y<0 then
				bg_status=2
				coin.position.y=256
				coin.position.x=60
				coin.velocity.y=-6.5
			end
		end
	)
	update_hand_position()
	update_coin(coin)
end

function draw_game()
	draw_background()
	draw_coin(coin)
	draw_hand(hand)
	draw_clouds()
	draw_baloons()
	print("score: "..score,0,0,7)
	if bg_status==1 then
		draw_progress()
	end
end
