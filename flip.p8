-- coin

function init_coin()
	gravity=0.1
	front_flip=create_animation({
		create_texture(),
		create_texture({x=8}),
		create_texture({x=16}),
		create_texture({x=24}),
		create_texture({x=32}),
		create_texture({x=40}),
		create_texture({x=48}),
		create_texture({x=56}),
	},8,true)
	lateral_flip=create_animation({
		create_texture({x=16}),
		create_texture({y=8}),
		create_texture({x=8,y=8}),
		create_texture({x=16,y=8}),
		create_texture({x=24,y=8}),
		create_texture({x=32,y=8}),
		create_texture({x=40,y=8}),
		create_texture({x=48,y=8}),
	},8,true)
end

function create_coin(position,shape)
	return {
		position=position,
		shape={
			width=8,
			height=8,
		},
		velocity={x=0,y=0},
		animation=lateral_flip,
		hide=false,
	}
end

function update_coin(coin)
	update_object(coin)
	coin.position.x=min(
		max(coin.position.x,0),
		120
	)
	if bg_status==0 then
		if coin.position.y<64 then
			coin.velocity.y+=gravity
			coin.hide=false
		else
			coin.velocity.y=0
			coin.hide=true
			f=false
		end
	elseif bg_status==1 then
		if coin.position.y<32 
			or (#baloons<=0 and #clouds<=0) then
			coin.velocity.y+=gravity
		else
			coin.velocity.y*=0.75
		end
	elseif bg_status==2 then
		coin.velocity.y+=gravity
		coin.hide=false
		if coin.position.y>128 and coin.velocity.y>0 then
			bg_status=1
			coin.position.y=-64
			coin.velocity.y=0
			init_clouds(5)
			init_baloon(3)
			init_progress(60)
		end
	end
	
	if bg_status==2 and coin.velocity.y<1 then
		coin.animation=front_flip
	else
		coin.animation=lateral_flip
	end
end

function draw_coin(coin)
	if not coin.hide then
		draw_object(coin)
	end
end
