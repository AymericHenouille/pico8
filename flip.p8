-- animation

function create_texture(options)
	if(options==nil)options={}
	return {
		x=options.x==nil and 0 or options.x,
		y=options.y==nil and 0 or options.y,
		width=options.width==nil and 8 or options.width,
		height=options.height==nil and 8 or options.height,	
	}
end

function create_animation(textures,speed,loop)
	return {
		start=time(),
		textures=textures==nil and {} or textures,
		speed=speed==nil and 1 or speed,
		loop=loop==nil and false or loop,
	}
end

function update_object(obj)
	if obj.velocity!=nil then
		local x=obj.velocity.x==nil and 0 or obj.velocity.x
		local y=obj.velocity.y==nil and 0 or obj.velocity.y 
		obj.position.x+=x
		obj.position.y+=y
	end
end

function draw_object(obj)
 if(obj.position==nil)obj.position={}
 if(obj.animation==nil)obj.animation={}
 if(obj.animation.textures==nil)obj.animation.textures={}
 if(obj.animation.speed==nil)obj.animation.speed=1
 local dt=time()-obj.animation.start
 dt*=obj.animation.speed
 dt=flr(dt)
 local frame=obj.animation.loop==true
 	and dt%#obj.animation.textures
  or min(dt,#obj.animation.textures-1)
 local texture=obj.animation.textures[frame+1]
 sspr(
 	texture.x,
 	texture.y,
 	texture.width,
 	texture.height,
 	obj.position.x==nil and 0 or obj.position.x,
 	obj.position.y==nil and 0 or obj.position.y,
 	obj.shape.width==nil and texture.width or obj.shape.width,
 	obj.shape.height==nil and texture.height or obj.shape.height,
 	texture.flip_width==nil and false or texture.flip_width,
 	texture.flip_height==nil and false or texture.flip_height
 )
 return texture
end
