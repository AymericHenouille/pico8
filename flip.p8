function create_baloon()
	return {
		x=flr(rnd()*120),
		y=128+flr(rnd()*128),
		texture=rnd({208,209,210,211}),
		speed=1+rnd()*1.1
	}
end

function init_baloon(numbers)
	start=time()
	baloons={}
	for index=1,numbers do
		add(
			baloons,
			create_baloon(),
			index
		)
	end
end

function update_baloons(coin,percent,colide_fn)
	for baloon in all(baloons) do
		baloon.y-=baloon.speed
		if not (
			coin.position.x>baloon.x+8
			or coin.position.y>baloon.y+8
			or coin.position.x+8<baloon.x
			or coin.position.y+8<baloon.y
		) then
			del(baloons,baloon)
			add(baloons,create_baloon()) 
			colide_fn()
		end
		if baloon.y<-8 then
			del(baloons,baloon)
			if percent<100 then
				add(baloons,create_baloon()) 
			end
		end
		if percent>=100 then
			if baloon.y>128 then
				del(baloons,baloon)
			end
		end
	end
end

function draw_baloons()
	if draw_baloons!=nil then
		for baloon in all(baloons) do
			spr(
				baloon.texture,
				baloon.x,
				baloon.y
			)
		end
	end
end
