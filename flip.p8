-- clouds

function create_cloud()
	return {
		x=flr(rnd()*120),
		y=128+flr(rnd()*128),
		texture=rnd({
			{x=112,y=0},
			{x=120,y=0},
			{x=112,y=8},
			{x=120,y=8}
		}),
		speed=1+rnd()*2
	}
end

function init_clouds(numbers)
	clouds={}
	for index=1,numbers do
		add(
			clouds,
			create_cloud(),
			index
		)
	end
end

function update_clouds(coin,percent,colide_fn)
	for cloud in all(clouds) do
		cloud.y-=cloud.speed
		if not (
			coin.position.x>cloud.x+16
			or coin.position.y>cloud.y+16
			or coin.position.x+8<cloud.x
			or coin.position.y+8<cloud.y
		) then
			del(clouds,cloud)
			add(clouds,create_cloud()) 
			colide_fn()
		end
		if cloud.y<-16 then
			del(clouds,cloud)
			if percent<100 then
				add(clouds,create_cloud()) 
			end
		end
		if percent>=100 then
			if cloud.y>128 then
				del(clouds,cloud)
			end
		end
	end
end

function draw_clouds()
	if clouds!=nil then
		for cloud in all(clouds) do
			sspr(
				cloud.texture.x,
				cloud.texture.y,
				8,8,
				cloud.x,
				cloud.y,
				16,16
			)
		end
	end
end
