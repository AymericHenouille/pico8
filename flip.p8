-- progress

function init_progress(duration)
	p_start=time()
	p_duration=duration
	percent=0
end

function update_progress()
	if p_start!=nil and p_duration!=nil then
		percent=min(100,((time()-p_start)/p_duration)*100)
		return percent
	end
	return 0
end

function draw_progress()
	local py=(percent/100)*59
	rectfill(0,32,5,96,12)
	rectfill(0,32,5,36,1)
	rectfill(0,90,5,96,3)
	rect(0,32,5,96,6)
	spr(23,-1,31+py)
end
