-- background

function init_background()
	bg_status=0
	is_space=false
	is_sky=false
	is_ground=true
	sky_offset=-128
	ground_offset=0
	offset_speed=5
end

function update_background(sp,sk,gr)
	bg_status=min(max(bg_status,0),2)
	if bg_status==0 then -- ground
		sky_offset=max(sky_offset-offset_speed,-128)
		if sky_offset==-128 then
			ground_offset=max(ground_offset-offset_speed,0)
			if ground_offset==0 then
				gr()
			end
		end
	elseif bg_status==1 then -- sky
		ground_offset=min(ground_offset+offset_speed,128)
		sky_offset=max(sky_offset-offset_speed,-128)
		if ground_offset==128 and sky_offset==-128 then
			sk()
		end
	elseif bg_status==2 then -- space
		ground_offset=min(ground_offset+offset_speed,128)
		if ground_offset==128 then
			sky_offset=min(sky_offset+offset_speed,0)
			if sky_offset==0 then
				sp()
			end
		end
	end
end

function draw_background()
	map(0,0,0,sky_offset,16,32)
	map(16,0,0,ground_offset,16,16)
end
